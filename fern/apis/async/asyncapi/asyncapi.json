{
    "asyncapi": "2.6.0",
    "info": {
        "title": "Active Symbols API",
        "version": "1.0.0"
    },
    "servers": {
        "production": {
            "url": "wss://ws.jambonzws.com/websockets/v3",
            "protocol": "wss"
        }
    },
    "channels": {
        "/": {
            "description": "Place a new order or interact with an existing one.",
            "bindings": {
                "ws": {
                    "query": {
                        "type": "object",
                        "properties": {
                            "app_id": {
                                "type": "string",
                                "title": "App ID",
                                "description": "App ID used for authenticating the client."
                            }
                        },
                        "required": [
                            "app_id"
                        ]
                    }
                }
            },
            "subscribe": {
                "message": {
                    "oneOf": [
                        {
                            "$ref": "#/components/messages/Ack"
                        },
                        {
                            "$ref": "#/components/messages/Command"
                        }
                    ]
                }
            },
            "publish": {
                "message": {
                    "oneOf": [
                        {
                            "$ref": "#/components/messages/CallStatus"
                        },
                        {
                            "$ref": "#/components/messages/VerbHook"
                        },
                        {
                            "$ref": "#/components/messages/VerbStatus"
                        },
                        {
                            "$ref": "#/components/messages/JambonzError"
                        },
                        {
                            "$ref": "#/components/messages/SessionNew"
                        },
                        {
                            "$ref": "#/components/messages/SessionRedirect"
                        },
                        {
                            "$ref": "#/components/messages/SessionReconnect"
                        }
                    ]
                }
            },
            "x-fern-examples": [
                {
                    "messages": [
                        {
                            "type": "publish",
                            "messageId": "SessionNew",
                            "value": {
                                "type": "session:new",
                                "msgid": "123",
                                "call_sid": "123",
                                "b3": "123",
                                "data": {
                                    "foo": "bar"
                                }
                            }
                        },
                        {
                            "type": "subscribe",
                            "messageId": "Ack",
                            "value": {
                                "type": "ack",
                                "msgid": "123",
                                "data": [
                                    {
                                    "foo": "bar"
                                    }
                                ]
                            }
                        },
                        {
                            "type": "publish",
                            "messageId": "VerbHook",
                            "value": {
                                "type": "verb:hook",
                                "msgid": "123",
                                "call_sid": "123",
                                "b3": "123",
                                "hook": "https://example.com/hook"
                            }
                        }
                    ]
                }
            ]
        }
    },
    "components": {
        "messages": {
            "SessionNew": {
                "name": "SessionNew",
                "payload": {
                    "$ref": "#/components/schemas/SessionNew"
